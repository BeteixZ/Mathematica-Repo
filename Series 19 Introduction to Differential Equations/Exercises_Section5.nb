Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"sln5", "=", 
  RowBox[{"AsymptoticDSolveValue", "[", 
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{"x", " ", 
       RowBox[{
        RowBox[{"y", "''"}], "[", "x", "]"}]}], "+", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"1", "-", "x"}], ")"}], 
       RowBox[{
        RowBox[{"y", "'"}], "[", "x", "]"}]}], "+", 
      RowBox[{"y", "[", "x", "]"}]}], "==", "0"}], ",", 
    RowBox[{"y", "[", "x", "]"}], ",", 
    RowBox[{"{", 
     RowBox[{"x", ",", "0", ",", "4"}], "}"}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.842707453933848*^9, 3.842707507300302*^9}, 
   3.8427204837920275`*^9},
 CellLabel->"In[83]:=",ExpressionUUID->"03682c52-d323-426d-a73a-bdcc28df46ee"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"(", 
    RowBox[{"1", "-", "x"}], ")"}], " ", 
   TemplateBox[{"1"},
    "C"]}], "+", 
  RowBox[{
   TemplateBox[{"2"},
    "C"], " ", 
   RowBox[{"(", 
    RowBox[{
     RowBox[{"3", " ", "x"}], "-", 
     FractionBox[
      SuperscriptBox["x", "2"], "4"], "-", 
