8aa49f80addd"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox[
     RowBox[{"L", 
      RowBox[{"{", 
       RowBox[{"g", "(", "t", ")"}], "}"}]}], 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"2", "+", "s"}], ")"}], "2"]], "=", 
    RowBox[{"L", 
     RowBox[{"{", 
      RowBox[{"t", " ", 
       SuperscriptBox["e", 
        RowBox[{
         RowBox[{"-", "2"}], " ", "t"}]]}], "}"}], "L", 
     RowBox[{"{", 
      RowBox[{"g", "(", "t", ")"}], "}"}]}]}], TraditionalForm]],
  ExpressionUUID->"4ef963e5-03ea-4dfc-bb81-d736ccf2d4db"]
