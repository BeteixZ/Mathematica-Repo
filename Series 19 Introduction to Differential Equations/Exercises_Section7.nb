Cell["\<\
The solution to the initial value problem can be obtained by taking the \
inverse Laplace transform of both sides of the equation. \
\>", "Text",
 CellChangeTimes->{{3.843149505408252*^9, 
  3.8431495274584517`*^9}},ExpressionUUID->"404dc751-ac87-4400-aa32-\
66199f44aa54"],

Cell["Recognize that:", "Text",
 CellChangeTimes->{{3.8431495327567844`*^9, 
  3.843149539759739*^9}},ExpressionUUID->"d8b3e691-e6a1-4714-bd05-\
6afa496ec592"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox[
     RowBox[{"L", 
      RowBox[{"{", 
       RowBox[{"g", "(", "t", ")"}], "}"}]}], 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"3", "+", "s"}], ")"}], "2"]], "=", 
    RowBox[{"L", 
     RowBox[{"{", 
      RowBox[{"t", " ", 
       SuperscriptBox["e", 
        RowBox[{
         RowBox[{"-", "3"}], " ", "t"}]]}], "}"}], "L", 
     RowBox[{"{", 
      RowBox[{"g", "(", "t", ")"}], "}"}]}]}], TraditionalForm]],
  ExpressionUUID->"5daa0e86-4841-4144-827c-4a8d5271366f"]
}], "Text",
 CellChangeTimes->{{3.8431495437076387`*^9, 3.8431495762136273`*^9}, {
  3.8431500643588095`*^9, 3.8431500649612646`*^9}, {3.843150324088887*^9, 
  3.843150334361377*^9}},ExpressionUUID->"8cca8a19-3786-4331-9f14-\
a4640faaa94c"],

Cell[TextData[{
 "Since ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"L", 
     RowBox[{"{", 
      RowBox[{"t", " ", 
       SuperscriptBox["e", 
        RowBox[{
         RowBox[{"-", "3"}], " ", "t"}]]}], "}"}], "L", 
     RowBox[{"{", 
      RowBox[{"g", "(", "t", ")"}], "}"}]}], "=", 
    RowBox[{"L", 
     RowBox[{"{", 
      RowBox[{"t", " ", 
       RowBox[{
        SuperscriptBox["e", 
         RowBox[{
          RowBox[{"-", "3"}], " ", "t"}]], "\[CircleTimes]", 
        RowBox[{"g", "(", "t", ")"}]}]}], "}"}]}]}], TraditionalForm]],
  ExpressionUUID->"6cef9dcf-f6e3-410f-b065-b4ccd85eaa72"],
 ", the inverse transform is ",
 Cell[BoxData[
  FormBox[
   RowBox[{"t", " ", 
    RowBox[{
     SuperscriptBox["e", 
      RowBox[{
       RowBox[{"-", "3"}], " ", "t"}]], "\[CircleTimes]", 
     RowBox[{"g", "(", "t", ")"}]}]}], TraditionalForm]],ExpressionUUID->
  "f98e208a-d3a2-4e38-b689-4809a3c6f6af"],
 "."
}], "Text",
 CellChangeTimes->{{3.8389212365581026`*^9, 3.8389212915930433`*^9}, {
  3.838921341369953*^9, 3.838921341369953*^9}, {3.843142010561721*^9, 
  3.8431420294398813`*^9}, {3.8431490899605527`*^9, 3.8431491096125517`*^9}, {
  3.8431495990083976`*^9, 3.843149603711753*^9}, {3.8431500788903008`*^9, 
  3.8431500888411427`*^9}, {3.8431505338671722`*^9, 3.8431505411588683`*^9}, {
  3.843228706436357*^9, 
  3.843228709868804*^9}},ExpressionUUID->"b74ac78c-816b-4042-96f4-\
5009378f649f"],

Cell[TextData[{
 "For example, if ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"g", "(", "t", ")"}], "=", 
    RowBox[{"cosh", "(", "t", ")"}]}], TraditionalForm]],ExpressionUUID->
  "5df9efbe-6844-4cdf-a511-02b7cccc77ce"],
 " then the solution to the initial value problem can be obtained by \
calculating ",
 Cell[BoxData[
  FormBox[
   RowBox[{"t", " ", 
    RowBox[{
     SuperscriptBox["e", 
      RowBox[{
       RowBox[{"-", "3"}], " ", "t"}]], "\[CircleTimes]", 
     RowBox[{"cosh", "(", "t", ")"}]}]}], TraditionalForm]],ExpressionUUID->
  "8a1f1266-4dce-4278-a45c-f62174948041"],
 ":"
}], "Text",
