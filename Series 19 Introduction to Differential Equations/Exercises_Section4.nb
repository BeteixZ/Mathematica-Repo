Cell[TextData[{
 "First, calculate the solution with ",
 ButtonBox["DSolveValue",
  BaseStyle->"Hyperlink",
  ButtonData->{
    URL["https://reference.wolfram.com/language/ref/DSolveValue.html"], None},
  ButtonNote->"https://reference.wolfram.com/language/ref/DSolveValue.html"],
 ":"
}], "Text",
 CellChangeTimes->{{3.8423640452516813`*^9, 3.842364052040368*^9}, {
  3.842439546561226*^9, 
  3.842439546562354*^9}},ExpressionUUID->"766baa0c-c33c-45ed-8ac0-\
89b533ece8e4"],

Cell[BoxData[
 RowBox[{"dSol", "=", 
  RowBox[{
   RowBox[{"DSolveValue", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{
          FractionBox["1", "32"], 
          RowBox[{
           RowBox[{"y", "''"}], "[", "x", "]"}]}], "+", 
         RowBox[{"2", 
          RowBox[{
           RowBox[{"y", "'"}], "[", "x", "]"}]}], "+", 
         RowBox[{"6", 
          RowBox[{"y", "[", "x", "]"}]}]}], "==", 
        RowBox[{"Exp", "[", 
         RowBox[{"-", "x"}], "]"}]}], ",", 
       RowBox[{
        RowBox[{"y", "[", "0", "]"}], "==", 
        RowBox[{"-", 
         FractionBox["1", "2"]}]}], ",", 
       RowBox[{
        RowBox[{
         RowBox[{"y", "'"}], "[", "0", "]"}], "==", "0"}]}], "}"}], ",", 
     RowBox[{"y", "[", "x", "]"}], ",", "x"}], "]"}], "//", 
   "FullSimplify"}]}]], "Input",
 CellChangeTimes->{{3.8423629970733147`*^9, 3.842363055530265*^9}, {
   3.8423632231968603`*^9, 3.842363224400361*^9}, 3.8423644792033544`*^9},
