 CellLabel->"In[7]:=",ExpressionUUID->"68fa981a-d41e-4e46-8dd9-659b2ce82916"],

Cell["\<\
The general solution is a sum of the particular and complementary solutions:\
\>", "Text",
 CellChangeTimes->{{3.842349371790547*^9, 3.8423493775367575`*^9}, {
  3.842349411095316*^9, 
  3.8423494168388844`*^9}},ExpressionUUID->"9e10c6f0-3d94-4fa7-987a-\
fd9f0d5484c2"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["y", "gen"], "(", "x", ")"}], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["\[ConstantC]", "1"], 
      SuperscriptBox["e", 
       RowBox[{"-", "x"}]]}], "+", 
     RowBox[{
      SubscriptBox["\[ConstantC]", "2"], "x", " ", 
      SuperscriptBox["e", 
       RowBox[{"-", "x"}]]}], "+", "6", "-", 
     RowBox[{"4", " ", "x"}], "+", 
     SuperscriptBox["x", "2"], "-", 
     FractionBox[
      RowBox[{"3", " ", 
       RowBox[{"cos", "(", "x", ")"}]}], "2"]}]}], TraditionalForm]],
  ExpressionUUID->"f8799078-6fe1-412e-8af5-a18833dd191f"]
}], "Text",
 CellChangeTimes->{{3.8423494303746996`*^9, 3.842349474427754*^9}, {
